<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextProgressBar">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="MaterialProgress">
        <attr name="progressIndeterminate" format="boolean" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="float" />
        <attr name="barSpinCycleTime" format="integer" />
        <attr name="circleRadius" format="dimension" />
        <attr name="fillRadius" format="boolean" />
        <attr name="barWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CollapsingTitleLayout">
        <attr name="expandedMargin" format="reference|dimension" />
        <attr name="expandedMarginStart" format="reference|dimension" />
        <attr name="expandedMarginBottom" format="reference|dimension" />
        <attr name="expandedMarginEnd" format="reference|dimension" />
        <attr name="expandedTextSize" format="reference|dimension" />
        <attr name="collapsedTextSize" format="reference|dimension" />
        <attr name="android:textAppearance" />
        <attr name="textSizeInterpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="CollapsingTextAppearance">
        <attr name="android:textSize" />
        <attr name="android:textColor" />
    </declare-styleable>

    <declare-styleable name="LinearListView">
        <attr name="android:entries" />
        <attr name="dividerThickness" format="dimension" />
    </declare-styleable>

    <declare-styleable name="dashedLineView">
        <attr name="dashWidth" format="float"/>
        <attr name="dashGap" format="float"/>
        <attr name="dashColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="TriangleShape">
        <attr name="triangleType">
            <enum name="centerTop" value="0" />
            <enum name="leftBottom" value="1" />
            <enum name="rightBottom" value="2" />
            <enum name="centerBottom" value="3" />
            <enum name="leftTop" value="4" />
        </attr>
        <attr name="triangleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ProgressProfileView">
        <attr name="max" format="float"/>
        <attr name="progress" format="float"/>
        <attr name="backgroundRingSize" format="dimension"/>
        <attr name="progressRingSize" format="dimension"/>
        <attr name="backgroundRingColor" format="color|reference"/>
        <attr name="progressRingColor" format="color|reference"/>
        <attr name="progressRingOutline" format="boolean"/>
        <attr name="progressRingCap">
            <enum name="BUTT" value="0"/>
            <enum name="ROUND" value="1"/>
            <enum name="SQUARE" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="AchievementToggle">
        <attr name="at_text" format="string"/>
        <attr name="at_textSize" format="dimension"/>
        <attr name="at_textColor" format="color"/>
        <attr name="at_backgroundColor" format="color"/>
        <attr name="at_selected" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TimelineConnector">
        <attr name="timeline_lineSize" format="dimension"/>
        <attr name="timeline_lineColor" format="color"/>
        <attr name="timeline_orientation">
            <enum name="up" value="0"/>
            <enum name="down" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>

    <declare-styleable name="StickyScrollView">
        <attr name="stuckShadowHeight" format="dimension" />
        <attr name="stuckShadowDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="InkPageIndicator">
        <attr name="dotDiameter" format="dimension" />
        <attr name="dotGap" format="dimension" />
        <attr name="animationDuration" format="integer" />
        <attr name="pageIndicatorColor" format="color" />
        <attr name="currentPageIndicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ElevationShadowView">
        <attr name="shadowElevation" format="dimension"/>
        <attr name="shadowOrientation" format="enum">
            <enum name="left" value="0"/>
            <enum name="top" value="1"/>
            <enum name="right" value="2"/>
            <enum name="bottom" value="3"/>
            <enum name="top_left" value="4"/>
            <enum name="top_right" value="5"/>
            <enum name="bottom_left" value="6"/>
            <enum name="bottom_right" value="7"/>
        </attr>
        <attr name="emulateParallelLight" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TextViewLabel">
        <attr name="labelColor" format="color"/>
        <attr name="labelColorSelected" format="color"/>
        <attr name="labelCornerRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ChipCloud">
        <attr name="deselectedColor" format="color" />
        <attr name="deselectedFontColor" format="color" />
        <attr name="selectedColor" format="color" />
        <attr name="selectedFontColor" format="color" />
        <attr name="cornerRadius" format="dimension"/>
        <attr name="selectMode">
            <enum name="single" value="0" />
            <enum name="multi" value="1" />
            <enum name="required" value="2" />
            <enum name="none" value="3" />
        </attr>
        <attr name="labels" format="reference"/>
    </declare-styleable>

    <declare-styleable name="Chip">
        <attr name="chipLabelColor" format="color" />
        <attr name="chipLabelFontColor" format="color" />
        <attr name="chipLabelSelectedColor" format="color" />
        <attr name="chipLabelSelectedFontColor" format="color" />
        <attr name="chipCornerRadius" format="dimension"/>
    </declare-styleable>

</resources>